// main.c

#include <stdio.h>
#include <stdlib.h>
#include "filaSeq.h"

int main(){
    fila f1;
    int x;
    iniciar(&f1);
    printf("Fila vazia? %d\n", vazia(f1));
    printf("Fila cheia?: %d\n", cheia(f1));
    
   printf("Informe o item a ser inserido na fila: ");
	scanf("%d", &x);
	enqueue(&f1, x);
	printf("Informe o item a ser inserido na fila: ");
	scanf("%d", &x);
	enqueue(&f1, x);
	printf("Informe o item a ser inserido na fila: ");
	scanf("%d", &x);
	enqueue(&f1, x);
	printf("Informe o item a ser inserido na fila: ");
	scanf("%d", &x);
	enqueue(&f1, x);
	printf("Informe o item a ser inserido na fila: ");
	scanf("%d", &x);
	enqueue(&f1, x);
	printf("Informe o item a ser inserido na fila: ");
	scanf("%d", &x);
	enqueue(&f1, x);
	printf("Informe o item a ser inserido na fila: ");
	scanf("%d", &x);
	enqueue(&f1, x);
	mostrarFila(f1);
	x = dequeue(&f1);
	x = dequeue(&f1);
	x = dequeue(&f1);
	x = dequeue(&f1);
	x = dequeue(&f1);
	x = dequeue(&f1);
	printf("Mostrando a fila vazia\n");
	mostrarFila(f1);
	printf("Informe o item a ser inserido na fila: ");
	scanf("%d", &x);
	enqueue(&f1, x);
	printf("Fila vazia?: %d\n", vazia(f1));
	printf("Fila cheia?: %d\n", cheia(f1));
	mostrarFila(f1);
	return 0;
}


/*Obs: O main não é tão importante, a lógica da fila está mesmo na
implementação, já a interface é mais um resumo da implementação mais
a declaração de constantes,variaveis e protótipos de funções. */

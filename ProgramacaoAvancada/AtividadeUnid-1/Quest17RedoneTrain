#include <stdio.h>
#include <stdlib.h>

int main(){
    
    int a,b,c,aux;
    
    printf("Escreva tres numeros inteiros:\n");
    scanf("%d %d %d",&a,&b,&c);
    
    if(a>b){
        aux=a;
        a=b;
        b=aux;
    }
    
    if(a>c){
        aux=a;
        a=c;
        c=aux;
    }
    
    if(b>c){
        aux=b;
        b=c;
        c=aux;
    }
    
    if(a == b && b==c){
        printf("Valores iguais");
    }
    
    printf("\n%d %d %d", a,b,c);
    
    
    
    return 0;
}
